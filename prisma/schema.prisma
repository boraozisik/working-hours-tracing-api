// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model WorkingDay {
  id Int @id @default(autoincrement())
  day Int
  exitedAt DateTime @default(now())
  name String
  email String
  mobile  String?
  message  String?
  company String?
}


